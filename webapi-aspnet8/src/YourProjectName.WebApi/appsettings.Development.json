{
  "ConnectionStrings": {
    "YourProjectNameDb": "Host=localhost;Port=5432;Database=postgres;Username=postgres;Password=postgres;Pooling=true"
  },
  "RedisSettings": {
    "ConnectionString": "localhost:6379",
    "KeyPrefix": "dev"
  },
  "JwtSettings": {
    "Authority": "http://localhost:9000/realms/blackbit",
    "Issuer": "http://localhost:9000/realms/blackbit",
    "Audience": "account"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3} {TraceIdentifier}] {Message:lj} {NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  }
}
